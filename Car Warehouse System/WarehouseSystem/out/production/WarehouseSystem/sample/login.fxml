<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: linear-gradient(to bottom, #5fb9ff, #5fffaf)" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">
   <children>
      <Label layoutX="142.0" layoutY="84.0" text="Manufacturer Verification">
         <font>
            <Font name="Rockwell Bold" size="25.0" />
         </font>
      </Label>
      <Label layoutX="139.0" layoutY="161.0" text="Password :">
         <font>
            <Font name="Rockwell Bold" size="20.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" layoutX="268.0" layoutY="159.0" />
      <Button fx:id="submitButton" layoutX="267.0" layoutY="226.0" mnemonicParsing="false" onAction="#loginAction" text="Submit">
         <font>
            <Font name="Rockwell Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
